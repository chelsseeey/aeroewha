#include <Servo.h>

Servo myServo;

int speedValue = 120;     // 속도 (90=정지, 120~150 = 방향/속도)
int turnCount = 5;        // 
int oneTurnTime = 1850;    // 1850-정확히 5바퀴

void setup() {
  myServo.attach(9);
}

void loop() {

  for (int i = 0; i < turnCount; i++) {
    myServo.write(speedValue);
    delay(oneTurnTime);
  }

  myServo.write(90); 

  while(1) {}
}
